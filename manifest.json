{
  "name": "Turntable.fm Remix",
  "version": "0.2.0",
  "manifest_version": 2,
  "description": " turntable.fm",
  "content_scripts": [
    {
    "matches": ["http://turntable.fm/*"],
    "exclude_globs": [
      "http://turntable.fm/", 
      "http://turntable.fm/lobby", 
      "http://turntable.fm/about", 
      "http://turntable.fm/jobs", 
      "http://turntable.fm/privacy", 
      "http://turntable.fm/copyright", 
      "http://turntable.fm/terms", 
      "http://turntable.fm/static/*"
    ],
    "js": ["js/inject.js"],
    "run_at": "document_end"
    }
  ],
  "permissions": [
    "http://turntable.fm/*",
    "http://turntable.fm/",
    "https://turntable.fm/",
    "notifications"
  ],
  "web_accessible_resources": [
    "js/backbone.js",
    "js/main.js",
    "js/remix.js",
    "js/require.js",
  "js/underscore.js",
	"css/remix.css",
	"js/underscore.js",
	"img/dj-icon.png",
	"img/downvotes-icon.png",
	"img/fanOf-icon.png",
	"img/upvotes-icon.png"
  ],
}